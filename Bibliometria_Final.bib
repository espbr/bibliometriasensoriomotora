author={Bringoux, L. and Macaluso, T. and Sainton, P. and Chomienne, L. and Buloup, F. and Mouchnino, L. and Simoneau, M. and Blouin, J.},
title={Double-Step Paradigm in Microgravity: Preservation of Sensorimotor Flexibility in Altered Gravitational Force Field},
journal={Frontiers in Physiology},
year={2020},
volume={11},
doi={10.3389/fphys.2020.00377},
