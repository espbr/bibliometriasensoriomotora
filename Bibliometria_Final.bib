@ARTICLE{Aucie2020,
author={Aucie, Y. and Zhang, X. and Sargent, R. and Torres-Oviedo, G.},
title={Motorized Shoes Induce Robust Sensorimotor Adaptation in Walking},
journal={Frontiers in Neuroscience},
year={2020},
volume={14},
