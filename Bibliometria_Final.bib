@ARTICLE{Charalambous20181999,
author={Charalambous, C.C. and Alcantara, C.C. and French, M.A. and Li, X. and Matt, K.S. and Kim, H.E. and Morton, S.M. and Reisman, D.S.},
title={A single exercise bout and locomotor learning after stroke: physiological, behavioural, and computational outcomes},
