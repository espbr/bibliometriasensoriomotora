@ARTICLE{Dulude201773,
author={Dulude, G. and O'Connor, K. and Audet, J.-S. and Bedard, M.-A.},
title={Over facilitation of unadapted cognitive processes in obsessive compulsive disorder as assessed with the computerized mirror pointing task},
