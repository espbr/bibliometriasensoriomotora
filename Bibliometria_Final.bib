@ARTICLE{Chunduru2019186,
author={Chunduru, P. and Kim, S.-J. and Lee, H.},
title={Gait symmetric adaptation: Comparing effects of implicit visual distortion versus split-belt treadmill on aftereffects of adapted step length symmetry},
