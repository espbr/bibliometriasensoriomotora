@ARTICLE{Berger1997781,
author={Berger, M. and Mescheriakov, S. and Molokanova, E. and Lechner-Steinleitner, S. and Seguer, N. and Kozlovskaya, I.},
title={Pointing arm movements in short- and long-term spaceflights},
