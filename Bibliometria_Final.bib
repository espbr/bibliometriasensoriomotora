@ARTICLE{Festini20161385,
author={Festini, S.B. and Preston, S.D. and Reuter-Lorenz, P.A. and Seidler, R.D.},
title={Emotion and reward are dissociable from error during motor learning},
journal={Experimental Brain Research},
year={2016},
volume={234},
