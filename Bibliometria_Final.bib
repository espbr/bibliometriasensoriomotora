@ARTICLE{Mierau2009115,
author={Mierau, A. and Schneider, S. and Abel, T. and Askew, C. and Werner, S. and Strüder, H.K.},
title={Improved sensorimotor adaptation after exhaustive exercise is accompanied by altered brain activity},
journal={Physiology and Behavior},
year={2009},
